<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iestecapp">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/abstracticon"
        android:label="IESTEC 2020"
        android:roundIcon="@drawable/abstracticon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".InfoActivity"></activity>
        <activity android:name=".ItinerarioGralActivity" />
        <activity android:name=".ItinerarioHoyActivity" />
        <activity android:name=".ItinerarioOpcionActivity" />
        <activity android:name=".TicketActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>